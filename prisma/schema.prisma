datasource db {
  provider = "postgresql"
  url      = env("POSTGRES_PRISMA_URL")
}

generator client {
  provider = "prisma-client-js"
}

model Project {
  project_id   String  @id @default(uuid())
  projectName  String  @map("project_name")
  description  String?
  start_date   DateTime
  end_date     DateTime?
  status       String
  type         String
  created_at   DateTime @default(now())
  updated_at   DateTime @updatedAt
}